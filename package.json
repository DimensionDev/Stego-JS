  "license": "MIT",
  "author": "DimensionDev",
  "main": "./cjs/node.js",
  "module": "./esm/node.js",
  "browser": "./cjs/dom.js",
  "bin": "./cjs/cli/cli.js",
  "scripts": {
    "prebuild": "pnpm run clean",
